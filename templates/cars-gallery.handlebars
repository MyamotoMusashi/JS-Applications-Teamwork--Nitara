<div class="container-fluid">
    <div class="row">
        {{#each this}}
            <div class="col-xl-6">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-12">
                            <h3>{{Brand}} : {{Model}}</h3>
                        </div>
                    </div> 
                    <div class="row">
                        <div class="col-xl-7">
                            <img class="img-responive" src="{{Image}}" alt="">
                        </div>
                        <div class="col-xl-5">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-xl-5">брой места: </div>
                                    <div class="col-xl-7">{{Seats}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-5">гориво: </div>
                                    <div class="col-xl-7">{{Fuel}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-5">скорости: </div>
                                    <div class="col-xl-7">{{gear}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-5">разход: </div>
                                    <div class="col-xl-7">{{fuelConsumption}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-5">екстри: </div>
                                    <div class="col-xl-7">{{extras}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-5">застраховки: </div>
                                    <div class="col-xl-7">{{insurance}}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
    <div id="page-navigator">
    </div>
</div>